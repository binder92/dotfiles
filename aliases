# General
alias zshconfig="nano $HOME/Projects/Own/dotfiles/zshrc"
alias aliases="nano $HOME/Projects/OWn/dotfiles/aliases"

alias composer="COMPOSER_MEMORY_LIMIT=-1 composer"

alias update="$HOME/Projects/Own/dotfiles/update"
alias updates="nano $HOME/Projects/Own/dotfiles/update"

alias dirsize="du -h -c ./ | tail -1"

# Codes
alias codes="cd $HOME/Projects"
alias tmrw_codes="cd $HOME/Projects/TMRW"
alias own_codes="cd $HOME/Projects/Own"

# Artisan
alias pa="valet php artisan"
alias tinker="valet php artisan tinker"
alias routes="valet php artisan route:list"

# Yii
alias py="php yii"

# PhpUnit
alias lp="vendor/bin/phpunit"
alias lpf="vendor/bin/phpunit --filter"

# Git
alias gs="git status"
alias gl="git log"
alias nah="git reset --hard && git clean -df"

# AWS
alias aws_dir="cd ~/.aws/"
